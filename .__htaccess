<IfModule mod_rewrite.c>
RewriteEngine on

RewriteRule ^projects/becreative360?$ https://sammurphey.net/code/websites/becreative360$2 [L,R=301]
RewriteRule ^projects/becreative360/?$ https://sammurphey.net/code/websites/becreative360$2 [L,R=301]
RewriteRule ^projects/becreative360/index.html?$ https://sammurphey.net/code/websites/becreative360$2 [L,R=301]

RewriteRule ^projects/order-confirmations?$ https://sammurphey.net/code/websites/local-trash-hauling$2 [L,R=301]
RewriteRule ^projects/order-confirmations/?$ https://sammurphey.net/code/websites/local-trash-hauling$2 [L,R=301]
RewriteRule ^projects/order-confirmations/index.html?$ https://sammurphey.net/code/websites/local-trash-hauling$2 [L,R=301]

RewriteRule ^projects/master-demolition-inc?$ https://sammurphey.net/code/websites/master-demolition-inc$2 [L,R=301]
RewriteRule ^projects/master-demolition-inc/?$ https://sammurphey.net/code/websites/master-demolition-inc$2 [L,R=301]
RewriteRule ^projects/master-demolition-inc/index.html?$ https://sammurphey.net/code/websites/master-demolition-inc$2 [L,R=301]

RewriteRule ^projects/mdi-admin?$ https://sammurphey.net/code/websites/mdi-admin$2 [L,R=301]
RewriteRule ^projects/mdi-admin/?$ https://sammurphey.net/code/websites/mdi-admin$2 [L,R=301]
RewriteRule ^projects/mdi-admin/index.html?$ https://sammurphey.net/code/websites/mdi-admin$2 [L,R=301]

RewriteRule \ sitemap.xml https://sammurphey.net/sitemap.xml [L,R=301]

RewriteCond %{REQUEST_URI}  !(\.png|\.jpg|\.gif|\.jpeg|\.bmp|\.svg|\.otf|\.ttf|\.woff|\.woff2|\.php|\.css|\.js|\.json|\.xml)$
RewriteRule ^(.*)$ ./index.php [NC,L,QSA]

</IfModule>

<IfModule mod_expires.c>
  ExpiresActive On
  ExpiresByType image/jpg "access 1 year"
  ExpiresByType image/jpeg "access 1 year"
  ExpiresByType image/gif "access 1 year"
  ExpiresByType image/png "access 1 year"
  ExpiresByType text/css "access 1 month"
  ExpiresByType text/html "access 1 month"
  ExpiresByType application/pdf "access 1 year"
  ExpiresByType text/x-javascript "access 1 month"
  ExpiresByType application/x-shockwave-flash "access 1 month"
  ExpiresByType image/x-icon "access 1 year"
  ExpiresDefault "access 1 month"
</IfModule>

<IfModule mod_headers.c>
  <FilesMatch "\.(html|php|js|css|xml|gz)$">
    Header append Vary: Accept-Encoding
  </FilesMatch>
</IfModule>

<ifModule mod_gzip.c>
  mod_gzip_on Yes
  mod_gzip_dechunk Yes
  mod_gzip_item_include file .(html?|txt|css|js|php|pl)$
  mod_gzip_item_include handler ^cgi-script$
  mod_gzip_item_include mime ^text/.*
  mod_gzip_item_include mime ^application/x-javascript.*
  mod_gzip_item_exclude mime ^image/.*
  mod_gzip_item_exclude rspheader ^Content-Encoding:.*gzip.*
</ifModule>
